<h1>Public::Notifications#index</h1>
<p>Find me in app/views/public/notifications/index.html.erb</p>
<div class="container">
  <div class="row">
    <% notifications = @notifications.where.not(visitor_id: current_end_user.id) %>
    <% if notifications.exists? %>
      <%= render 'public/notifications/notification' %>
      <%= paginate @notifications %>
    <% end %>
  </div>
</div>